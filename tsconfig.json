{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@pages/*": ["./src/pages/*"],
      "@favoriets/*": ["./src/pages/Favoriets/*"],
      "@feed/*": ["./src/pages/Feed/*"],
      "@library/*": ["./src/pages/Library/*"],
      "@player/*": ["./src/pages/Player/*"],
      "@trending/*": ["./src/pages/Trending/*"],
      "@shared/*": ["./src/shared/*"],
      "@routing/*": ["./src/pages/Routing/*"],
      "@home/*": ["./src/pages/Home/*"],
      "@images/*": ["./src/images/*"],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "./index.d.ts"
  ]
}
